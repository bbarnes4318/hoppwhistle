<include>
  <gateway name="signalwire">
    <param name="realm" value="$${signalwire_domain}"/>
    <param name="proxy" value="$${signalwire_proxy}"/>
    <param name="username" value="$${signalwire_user}"/>
    <param name="password" value="$${signalwire_pass}"/>
    <param name="register" value="false"/>
    <param name="caller-id-in-from" value="true"/>
    <param name="ping" value="25"/>
  </gateway>
</include>
